    "TDL",
    "Test",
    "Driven",
    "Learning",
    "SyncPromise",
    "Parser"
  ],
  "author": "Jason Whitted <jason.whitted@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jason-whitted/tdl-syncpromise-parser/issues"
  },
  "homepage": "https://github.com/jason-whitted/tdl-syncpromise-parser#readme",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.2",
    "data-tada": "0.0.25",
    "expect": "^1.20.2",
    "jsdom": "^11.1.0",
    "mocha": "^3.5.0",
    "moment": "^2.18.1",
    "nyc": "^11.1.0",
    "react": "^15.6.1"
  }
